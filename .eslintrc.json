{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style":"off",
        "indent": ["error", 4],
        "class-methods-use-this":"off",
        "no-multi-str":"off",
        "import/no-cycle":"off",
        "max-len":"off",
        "import/prefer-default-export":"off",
        "import/no-mutable-exports":"off"
    }
}
